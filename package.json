{
  "name": "seds-iupui-website",
  "version": "0.0.1",
  "private": true,
  "description": "Official website of SEDS at IUPUI student organization",
  "author": "Chris Cardoza",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"next dev\" \"cp .env.local ./studio/.env.development && cd studio && sanity start\"",
    "build": "echo 'Building Sanity to public/studio...' && cd studio && sanity build ../public/studio -y && cd .. && next build",
    "start": "next start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@fontsource/inter": "^4.5.1",
    "@fontsource/nunito-sans": "^4.5.1",
    "@sanity/client": "^3.0.3",
    "@sanity/image-url": "^1.0.1",
    "framer-motion": "^6.5.0",
    "next": "^12.2.1",
    "next-sanity": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "17.x",
    "react-dom": "17.x",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.30.2",
    "@types/node": "^18.0.1",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cssnano": "^5.1.12",
    "lerna": "^5.1.8",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
